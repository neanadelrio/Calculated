{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,

    "noEmit": true
  },
}
